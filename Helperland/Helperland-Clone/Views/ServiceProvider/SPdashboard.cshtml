                        </div>
                        <!-- Tabs content -->
                    </div>


            <div id="newServiceRequests" class="tabcontent table-responsive">
                <table class="table" id="newServiceRequestTable">
                    <thead>
                      <tr>
                        <th scope="col">
                            <p>Service ID <img src="~/images/sort.png"></p>
                        </th>
                        <th scope="col">
                            <p>Service Date</p>
                        </th>
                        <th scope="col">
                            <p>Customer details </p>
                        </th>
                        <th scope="col">
                            <p>Payment </p>
                        </th>
                         <th scope="col">
                            <p>Time conflict</p>
                        </th>
                        <th scope="col">
                            <p>Actions</p>
                        </th>
                      </tr>
                    </thead>
                    <tbody>
                        @if (Model.Count() > 0)
                    {
                        @foreach (var item in Model)
                        {
                            @if (item.Status == 1)
                            {
                                <tr data-value=@item.ServiceId>
                                    <th scope="row" class="align-middle" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p>
                                            <span class="serviceId">@item.ServiceId</span>
                                        </p>
                                    </th>
                                    <td class="align-middle" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p>
                                            <img src="~/images/calendar2.png" style="margin-bottom: 7px"> 
                                            <b><span class="serviceDate">@item.Date</span></b><br>
                                            <img src="~/images/layer-14.png"> <span class="serviceTime">@item.StartTime - @item.EndTime</span>
                                        </p>
                                    </td>
                                    <td class="align-middle" data-lable="Customer Details" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p><span class="custName">@item.CustomerName</span><br>
                                            <img src="~/images/layer-15.png" style="vertical-align: text-bottom;">
                                            <span class="serviceAddress">@item.Address</span></p>
                                    </td>
                                    <td class="align-middle" data-lable="Payment" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p><span class="netAmt">@item.TotalCost</span> &euro;</p>
                                    </td>
                                    <td class="align-middle" data-lable="Time Conflict" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        @{
                                            string conflictIdtemp = "Conflict" + @item.ServiceId;
                                        }
                                        <a class="newReqConflictBtn d-none" id=@conflictIdtemp >Conflict</a>
                                    </td>
                                    <td class="align-middle">
                                        <button type="button" id="newServiceReqAccept" class="btn btn-outline-light" style="background-color: #1D7A8C; border-radius: 17px">Accept</button>
                                    </td>
                                    <td class="totleTime d-none">@item.Duration</td>
                                    <td class="extraCabinet d-none">@item.Cabinet</td>
                                    <td class="extraOven d-none">@item.Oven</td>
                                    <td class="extraWindow d-none">@item.Window</td>
                                    <td class="extraFridge d-none">@item.Fridge</td>
                                    <td class="extraLaundry d-none">@item.Laundry</td>
                                    <td class="serviceZip d-none">@item.ZipCode</td>
                                </tr>
                            }
                        }

                    }

                    </tbody>
                  </table>
            </div>
            
            <div id="Upcoming Services" class="tabcontent table-responsive">
                <div id="divStatusFailedBootstrapAlert">
                </div>
                <table class="table" id="upcomingServicesTable">
                    <thead>
                      <tr>
                        <th scope="col">
                            <p>Service ID <img src="~/images/sort.png"></p>
                        </th>
                        <th scope="col">
                            <p>Service Date <img src="~/images/sort.png"></p>
                        </th>
                        <th scope="col">
                            <p>Customer details <img src="~/images/sort.png"></p>
                        </th>
                        <th scope="col">
                            <p>Payment </p>
                        </th>
                        <th scope="col">
                            <p>Distance <img src="~/images/sort.png"></p>
                        </th>
                        <th scope="col">
                            <p>Actions <img src="~/images/sort.png"></p>
                        </th>
                      </tr>
                    </thead>
                    <tbody>
                    @if (Model.Count() > 0)
                    {
                        @foreach (var item in Model)
                        {
                            @if (item.Status == (int)ServiceRequestStatusEnum.IsComplete || item.Status == (int)ServiceRequestStatusEnum.Accepted)
                            {
                                <tr data-value=@item.ServiceId id=@item.ServiceId>
                                    <th scope="row" class="align-middle" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p><span class="serviceId">@item.ServiceId</span></p>
                                    </th>
                                    <td class="align-middle" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p><img src="~/images/calendar2.png" style="margin-bottom: 7px"> 
                                            <b><span class="serviceDate">@item.Date</span></b><br>
                                            <img src="~/images/layer-14.png"> <span class="serviceTime">@item.StartTime - @item.EndTime</span></p>
                                    </td>
                                    <td class="align-middle" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p><span class="custName">@item.CustomerName</span><br>
                                            <img src="~/images/layer-15.png" style="vertical-align: text-bottom;"> 
                                            <span class="serviceAddress">@item.Address</span></p>
                                    </td>
                                     <td class="align-middle" data-lable="Payment" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p><span class="netAmt">@item.TotalCost</span> &euro;</p>
                                    </td>
                                    <td class="align-middle" data-bs-toggle="modal" data-bs-target="#spServiceDetails">
                                        <p>15 km</p>
                                    </td>
                                    <td class="align-middle text-center">
                                        @if (item.Status == (int)ServiceRequestStatusEnum.IsComplete)
                                        {
                                            <button type="button" class="btn btn-outline-light completeService" style="background-color: #67B644; line-height: 1;" id="IsCompleteBtn" data-bs-toggle="modal" data-bs-target="#IsCompleteServiceModal">IsComplete?</button>
                                        }
                                        
                                        <button type="button" class="btn btn-outline-light cancelService" style="background-color: #FF6B6B; border-radius: 17px" id="CancelServiceBtn" data-bs-toggle="modal" data-bs-target="#cancelServiceModal">Cancel</button>
                                    </td>
                                    <td class="totleTime d-none">@item.Duration</td>
                                    <td class="extraCabinet d-none">@item.Cabinet</td>
                                    <td class="extraOven d-none">@item.Oven</td>
                                    <td class="extraWindow d-none">@item.Window</td>
                                    <td class="extraFridge d-none">@item.Fridge</td>
                                    <td class="extraLaundry d-none">@item.Laundry</td>
                                    <td class="serviceZip d-none">@item.ZipCode</td>
                                </tr>
                            }
                        }
                    }
                 
                    </tbody>
                  </table>
            </div>

            <div id="serviceHistory" class="tabcontent table-responsive">
                <div id="divStatusFailedBootstrapAlert">
                </div>
                <div class="Service-History d-flex align-items-center justify-content-between">
                    <div><h3>Service History</h3></div>
                    <div><button type="button" class="btn btn-outline-light">Export</button></div>
                </div>
                <table class="table" id="serviceHistoryTable">
