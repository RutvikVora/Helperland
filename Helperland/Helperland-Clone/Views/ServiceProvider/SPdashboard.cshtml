@using System.Security.Claims
@using Helperland_Clone.Enums
@model IEnumerable<Helperland_Clone.ViewModels.SPdashboardViewModel>;

@{
    ViewData["Title"] = "Upcoming Service";
    ViewBag.transparentNavbar = false;
    Layout = "_Layout";

    var userId = User.FindFirst(ClaimTypes.NameIdentifier).Value;
}   

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/ServiceProvider.js"></script>
}

@section Style{
    <link rel="stylesheet" href="~/css/UpcomingService.css">
    <link rel="stylesheet" href="~/css/Modals.css">
    <link rel="stylesheet" href="~/css/responsive.css">
    <link rel="stylesheet" href="~/css/DataTable.css">
}


<div class="welcoming text-center">
        <h3><span style="font-weight: normal">Welcome,</span>@User.FindFirst(ClaimTypes.Name).Value!</h3>
    </div>

    <section id="admin-panel">
