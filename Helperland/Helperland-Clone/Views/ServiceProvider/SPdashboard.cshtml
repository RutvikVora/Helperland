                <button class="tablinks" onclick="openTab(event, 'Upcoming Services')" id="defaultOpen">Upcoming Services</button><hr>
                <button class="tablinks" onclick="openTab(event, 'Upcoming Services')">Service Schedule</button><hr>
                <button class="tablinks" onclick="openTab(event, 'serviceHistory')">Service History</button><hr>
                <button class="tablinks" onclick="openTab(event, 'MyRating')" id="MyRatingTab">My Ratings</button><hr>
                <button class="tablinks" onclick="openTab(event, 'BlockCustomer')" id="BlockCustomerTab">Block Customer</button><hr>
